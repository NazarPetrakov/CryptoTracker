<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackground" Color="#212121" />
    <SolidColorBrush x:Key="SecondaryBackground" Color="#323232" />
    <SolidColorBrush x:Key="PrimaryForeground" Color="#13EDDB" />
    <SolidColorBrush x:Key="SecondaryForeground" Color="#0D7377" />

    <SolidColorBrush x:Key="PrimaryBackgroundHover" Color="#2b2b2b" />
    <SolidColorBrush x:Key="PrimaryBackgroundSelected" Color="#3b3b3b" />

    <SolidColorBrush x:Key="SecondaryBackgroundHover" Color="#424242" />
    <SolidColorBrush x:Key="SecondaryBackgroundSelected" Color="#4f4f4f" />
</ResourceDictionary>

