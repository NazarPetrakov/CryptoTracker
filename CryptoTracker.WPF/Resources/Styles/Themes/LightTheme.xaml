<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackground" Color="#F4F4F4" />
    <SolidColorBrush x:Key="SecondaryBackground" Color="#65EEB7" />
    <SolidColorBrush x:Key="PrimaryForeground" Color="#474744" />
    <SolidColorBrush x:Key="SecondaryForeground" Color="#82827C" />

    <SolidColorBrush x:Key="PrimaryBackgroundHover" Color="#F0F0F0" />
    <SolidColorBrush x:Key="PrimaryBackgroundSelected" Color="#EBEBEB" />

    <SolidColorBrush x:Key="SecondaryBackgroundHover" Color="#61E8B2" />
    <SolidColorBrush x:Key="SecondaryBackgroundSelected" Color="#5EE0AC" />
</ResourceDictionary>